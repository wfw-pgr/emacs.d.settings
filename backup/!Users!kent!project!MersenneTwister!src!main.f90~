program main
  use mt19937Mod
  implicit none
  integer :: seed

  ! ------------------------------------------------------ !
  ! --- [1] initialize                                 --- !
  ! ------------------------------------------------------ !
  seed = 10
  call sgrnd( seed )

  ! ------------------------------------------------------ !
  ! --- [2] generate samples                           --- !
  ! ------------------------------------------------------ !
  write(6,*)
  write(6,"(5(i12,1x))") grnd_int32(), grnd_int32(), grnd_int32(), grnd_int32(), grnd_int32()

end program main
