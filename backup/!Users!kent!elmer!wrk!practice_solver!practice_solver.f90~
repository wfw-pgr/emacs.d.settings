subroutine practice_solver( Solver, Model, dt, transient )
  use DefUtils
  implicit none
  type(Solver_t)              :: Solver
  type(Model_t)               :: Model
  real(kind=dp)               :: dt
  logical                     :: transient
  
  type(ValueList_t), pointer  :: listsol
  character(Len=Max_Name_Len) :: varname
  logical                     :: GotIt

  ! get list of Solver Section
  listsol => GetSolverParams()
  ! get keyword variable
  varname = GetString( listsol, "Variable", GotIt )
  
  if ( .not.GotIt ) then
     call FATAL( "MySolver", "Variable not found" )
  end if

  write(6,*) varname

  return
end subroutine practice_solver
