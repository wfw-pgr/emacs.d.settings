

def unpack_kw( arg1, kw1="kw1", kw2="kw2" ):
    print( "Hello, I am {0}, program to say {1}.".format(kw1,kw2) )
    return()


# ========================================================= #
# ===   Execution of Pragram                            === #
# ========================================================= #

if ( __name__=="__main__" ):
    unpack_kw()
